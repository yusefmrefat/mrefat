local function SJLLFVW_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(SJLLFVW_fake_script)()
local function RENVWF_fake_script() -- PlayerName.LocalScript 
	local script = Instance.new('LocalScript', PlayerName)

	if game.Players.LocalPlayer.Name == "Disunize" then
		script.Parent.Text = "Hello Tester "..game.Players.LocalPlayer.Name.. "!"
	elseif game.Players.LocalPlayer.Name == "4666realname6664" then
		script.Parent.Text = "Hello Owner Mrefat "..game.Players.LocalPlayer.Name.. "!"
	else
		script.Parent.Text = "Hello "..game.Players.LocalPlayer.Name.. "!"
	end
end
coroutine.wrap(RENVWF_fake_script)()
local function TYCNRD_fake_script() -- PlayerIcon.LocalScript 
	local script = Instance.new('LocalScript', PlayerIcon)

	script.Parent.Image = "http://www.roblox.com/Thumbs/Avatar.ashx?x=100&y=100&userId=" .. game.Players.LocalPlayer.UserId
end
coroutine.wrap(TYCNRD_fake_script)()
local function EFFLNET_fake_script() -- Du.Script 
	local script = Instance.new('Script', Du)

	function onClick(mouse)
	end
	
		value = 10
		while (value > 1) do
			game.Players.yusefk7878.Backpack.Rose.Parent = game.Workspace.yusefk7878
			game.Workspace.yusefk7878.Rose.Name = "mrefat"
			game.Workspace.yusefk7878.mrefat.Handle.Mesh:Destroy()
			game.Workspace.yusefk7878.mrefat.Parent = game.Workspace
			game.Workspace.yusefk7878.Humanoid.Health = 0
			value = value - 1
		wait(7)
		
		end
	script.Parent.MouseButton1Click:connect(onClick)
	
	
end
coroutine.wrap(EFFLNET_fake_script)()
