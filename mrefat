-- Scripts:

local function QXGD_fake_script() -- cross.GuiClick_Script 
	local script = Instance.new('LocalScript', cross)

	function OnClicked() 
	script.Parent.Parent.Parent:Destroy()
	end 
	
	script.Parent.MouseButton1Down:connect(OnClicked)
end
coroutine.wrap(QXGD_fake_script)()
local function OBKQ_fake_script() -- Intro.LocalScript 
	local script = Instance.new('LocalScript', Intro)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(OBKQ_fake_script)()
local function EUKV_fake_script() -- Button.GuiClick_Script 
	local script = Instance.new('LocalScript', Button)

	function OnClicked() 
		script.Parent.Parent.TopRightTitle.Text = "Verify"
		script.Parent.Parent.Title.Text = "Verifying You"
		wait(1)
		script.Parent.Parent.Title.Text = "Verifying You."
		wait(1)
		script.Parent.Parent.Title.Text = "Verifying You.."
		wait(1)
		script.Parent.Parent.Title.Text = "Verifying You..."
		if game.Players.LocalPlayer.Name == "Disunize" then
			wait(2)
			script.Parent.Parent.Title.Text = " thanks for using the hub, digit legend!"
			wait(3)
			script.Parent.Parent.Visible = false
			script.Parent.Parent.Parent.Main.Visible = true
		elseif game.Players.LocalPlayer.Name == "yusefk7878" then
			wait(2)
			script.Parent.Parent.Title.Text = "owner testing??!?!?"
			wait(3)
			script.Parent.Parent.Visible = false
			script.Parent.Parent.Parent.Main.Visible = true
		else
			wait(2)
			script.Parent.Parent.Title.Text = " Verified!"
			wait(3)
			script.Parent.Parent.Visible = false
			script.Parent.Parent.Parent.Main.Visible = true
		end
	end 
	
	script.Parent.MouseButton1Down:connect(OnClicked)
end
coroutine.wrap(EUKV_fake_script)()
